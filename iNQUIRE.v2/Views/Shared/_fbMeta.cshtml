@if (ViewBag.FacebookAppId != null)
{<meta property="fb:app_id" content="@ViewBag.FacebookAppId" />}

@if (ViewBag.ogTitle != null)
{<meta property="og:title" content="@ViewBag.ogTitle" />}

@if (ViewBag.ogImage != null)
{<meta property="og:image" content="@ViewBag.ogImage" />}

@if (ViewBag.ogImageWidth != null)
{<meta property="og:image:width" content="@ViewBag.ogImageWidth" />}

@if (ViewBag.ogImageHeight != null)
{<meta property="og:image:height" content="@ViewBag.ogImageHeight" />}

@if (ViewBag.ogUrl != null)
{<meta property="og:url" content="@ViewBag.ogUrl" />}

@if (ViewBag.ogType != null)
{<meta property="og:type" content="@ViewBag.ogType" />}

@if (ViewBag.ogDescription != null)
{<meta property="og:description" content="@ViewBag.ogDescription" />}
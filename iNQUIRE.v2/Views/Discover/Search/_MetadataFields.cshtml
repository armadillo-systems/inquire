<li id="metadata-source" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().Source)">
    <div>
        <span class="metadata-label">Source: </span>
        <span id="metadata-source-text" class="metadata-field-text" data-bind="html: resultCurrent().Source"></span>
    </div>
</li>

<li id="metadata-collection" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().Collection)">
    <div>

        <span id="metadata-collection-text" class="metadata-field-text" data-bind="html: resultCurrent().Collection"></span>
    </div>
</li>

<li id="metadata-collection-history" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().CollectionHistory)">
    <div>
        <span class="metadata-label">History: </span>
        <ul class="keywords metadata-field-text" data-bind="foreach: resultCurrent().CollectionHistory">
            <li class="keyword"><a data-bind="html: $data, click: $root.keywordClick" class="keyword"></a></li>
        </ul>
    </div>
</li>

<li id="metadata-format" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().Format)">
    <div>
        <span class="metadata-label">Format: </span>
        <span id="metadata-title-text" class="metadata-field-text" data-bind='html: resultCurrent().Format'></span>
    </div>
</li>

<li id="metadata-repository" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().Repository)">
    <div>
        <span class="metadata-label">Repository: </span>
        <span id="metadata-title-text" class="metadata-field-text" data-bind='html: resultCurrent().Repository'></span>
    </div>
</li>

<li id="metadata-page" class="metadata-field" data-bind="visible: !IsFieldNullOrEmpty(resultCurrent().Page)">
